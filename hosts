# Example of hosts file

[all]
mid-docker01.lab1.midgar.local
mid-docker02.lab1.midgar.local
mid-consul01.lab1.midgar.local
mid-consul02.lab1.midgar.local
mid-consul03.lab1.midgar.local

[control]

[docker_swarm_manager]
mid-docker01.lab1.midgar.local

[docker_swarm]
mid-docker01.lab1.midgar.local
mid-docker02.lab1.midgar.local

[consul_server]
mid-consul02.lab1.midgar.local
mid-consul03.lab1.midgar.local

[consul_server_bootstrap]
mid-consul01.lab1.midgar.local

[consul_client]
mid-docker01.lab1.midgar.local
mid-docker02.lab1.midgar.local
